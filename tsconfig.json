{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "plugins": [
      {
        "name": "@styled/typescript-styled-plugin"
      }
    ],
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@navigations": [
        "./src/navigations"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@screens": [
        "./src/screens"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@dtos/*": [
        "./src/dtos/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}
